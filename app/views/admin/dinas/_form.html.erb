<%= simple_form_for(@dinas, url: (@dinas.new_record? ? admin_dinas_path(@dinas) : admin_dina_path(@dinas))) do |f| %>
  <!-- <%= f.error_notification %> -->
  <div class="card-panel">
    <div class="row">
      <div class="form-inputs">
        <div class="col m12 s12">
          <div class="row">
            <%= f.input :code, required: true, label: 'Kode', autofocus: true, wrapper_html: { class: 'm6' } %>
            <%= f.input :name, required: true, label: 'Name', autofocus: true, wrapper_html: { class: 'm6' } %>
          </div>
          <div class="row">
            <%= f.input :address, required: true %>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col m12">
    <div class="right" style="padding:20px;">
      <button class="btn cyan waves-effect waves-light right" type="submit">
        SIMPAN DINAS
        <i class="material-icons right">send</i>
      </button>
    </div>
    <div class="right" style="padding:20px;">
      <%= link_to 'Back', admin_users_path, class:"btn cyan waves-effect waves-light right" %>
    </div>
  </div>
  <% end %>